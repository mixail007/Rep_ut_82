
Процедура ПередЗаписью(Отказ)
	# Если Клиент Тогда
	Для каждого СтрСписок из СписокПараметров Цикл
		Если ТипЗнч(СтрСписок.Параметр) = Тип("Строка") и ЗначениеНеЗаполнено(СтрСписок.ТипПараметра) Тогда
			Сообщить("Необходимо заполнить тип параметра");
			Отказ = Истина;
		КонецЕсли;
	КонецЦикла;
	ТЗПараметры = СписокПараметров.Выгрузить();
	ТЗПараметры.Свернуть("ВидСравнения, ТипПараметра");
	Для Каждого СтрПар из ТЗПараметры Цикл
		Отбор = Новый Структура;
		Отбор.Вставить("ВидСравнения", СтрПар.ВидСравнения);
		Отбор.Вставить("ТипПараметра", СтрПар.ТипПараметра);
		Строки = СписокПараметров.НайтиСтроки(Отбор);
		Если Строки.Количество() >1 и (СтрПар.ВидСравнения = "Содержит" или СтрПар.ВидСравнения = "Не содержит")  Тогда
			Сообщить("Можно добавить лишь одно сравнение " + СтрПар.ВидСравнения);
			Отказ = Истина;
		КонецЕсли;
	КонецЦикла;
	# КонецЕсли
КонецПроцедуры
