
Процедура КоманднаяПанель2Заполнить(Кнопка)
		Запрос = Новый Запрос;
	Запрос.Текст = 
		"ВЫБРАТЬ РАЗЛИЧНЫЕ
		|	ПродажиОбороты.Номенклатура.Производитель КАК Производитель
		|ИЗ
		|	РегистрНакопления.Продажи.Обороты(&Нач, &Кон, , ) КАК ПродажиОбороты
		|ГДЕ
		|	ПродажиОбороты.Номенклатура.НоменклатурнаяГруппа В(&Список)";

	Запрос.УстановитьПараметр("Нач",ДобавитьМесяц(текущаяДата(),-12) );
	Запрос.УстановитьПараметр("Кон",КонецДня(текущаяДата()));
	Запрос.УстановитьПараметр("Список", НоменклатурныеГруппы.выгрузитьКолонку("НоменклатурнаяГруппа"));
	
	Результат = Запрос.Выполнить().Выгрузить();

    производители.Загрузить(Результат);

КонецПроцедуры
