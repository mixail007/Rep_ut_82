<?xml version="1.0" encoding="UTF-8"?>
<DataCompositionSchema xmlns="http://v8.1c.ru/8.1/data-composition-system/schema" xmlns:dcscom="http://v8.1c.ru/8.1/data-composition-system/common" xmlns:dcscor="http://v8.1c.ru/8.1/data-composition-system/core" xmlns:dcsset="http://v8.1c.ru/8.1/data-composition-system/settings" xmlns:v8="http://v8.1c.ru/8.1/data/core" xmlns:v8ui="http://v8.1c.ru/8.1/data/ui" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<dataSource>
		<name>ИсточникДанных1</name>
		<dataSourceType>Local</dataSourceType>
	</dataSource>
	<dataSet xsi:type="DataSetQuery">
		<name>НаборДанных1</name>
		<field xsi:type="DataSetFieldField">
			<dataPath>Регион</dataPath>
			<field>Регион</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Регион</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Важность</dataPath>
			<field>Важность</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Важность</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДоговорКонтрагента</dataPath>
			<field>ДоговорКонтрагента</field>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Сделка</dataPath>
			<field>Сделка</field>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Поле1</dataPath>
			<field>Поле1</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Поле2</dataPath>
			<field>Поле2</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>НаправлениеПродаж</dataPath>
			<field>НаправлениеПродаж</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДопустимаяСуммаЗадолженности</dataPath>
			<field>ДопустимаяСуммаЗадолженности</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДебиторскаяЗадолженность</dataPath>
			<field>ДебиторскаяЗадолженность</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ПросроченнаяДебиторскаяЗадолженность</dataPath>
			<field>ПросроченнаяДебиторскаяЗадолженность</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДоговорКонтрагентаОтветственноеЛицо</dataPath>
			<field>ДоговорКонтрагентаОтветственноеЛицо</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Покупатель</dataPath>
			<field>Покупатель</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СреднееДнейПросрочки</dataPath>
			<field>СреднееДнейПросрочки</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СреднееДолгКлиента</dataPath>
			<field>СреднееДолгКлиента</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СреднееПроцентПДЗОтОбщейСуммы</dataPath>
			<field>СреднееПроцентПДЗОтОбщейСуммы</field>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ВзаиморасчетыСКонтрагентамиОбороты.Сделка,
	ВзаиморасчетыСКонтрагентамиОбороты.Сделка.ДатаОплаты КАК СделкаДатаОплаты
ПОМЕСТИТЬ ДатыОплатПлан
ИЗ
	РегистрНакопления.ВзаиморасчетыСКонтрагентами.Обороты(
			&amp;ТекДатаМинус1_5Года,
			,
			,
			ДоговорКонтрагента.Владелец = &amp;ТекКонтрагент
				И Сделка ССЫЛКА Документ.ЗаказПокупателя
				И Сделка.Дата &gt;= &amp;ТекДатаМинусГод
				И Сделка.ДатаОплаты &lt; &amp;ТекДата) КАК ВзаиморасчетыСКонтрагентамиОбороты

СГРУППИРОВАТЬ ПО
	ВзаиморасчетыСКонтрагентамиОбороты.Сделка,
	ВзаиморасчетыСКонтрагентамиОбороты.Сделка.ДатаОплаты
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ДатыОплатПлан.СделкаДатаОплаты КАК СделкаДатаОплаты,
	ДатыОплатПлан.Сделка КАК Сделка,
	СУММА(ВЫБОР
			КОГДА ВзаиморасчетыСКонтрагентамиОстаткиИОбороты.Период = &amp;ТекДатаМинусГод
				ТОГДА ВзаиморасчетыСКонтрагентамиОстаткиИОбороты.СуммаВзаиморасчетовКонечныйОстаток
			ИНАЧЕ ВЫБОР
					КОГДА ВзаиморасчетыСКонтрагентамиОстаткиИОбороты.Период &lt;= ДатыОплатПлан.СделкаДатаОплаты
						ТОГДА ВзаиморасчетыСКонтрагентамиОстаткиИОбороты.СуммаВзаиморасчетовОборот
					ИНАЧЕ 0
				КОНЕЦ
		КОНЕЦ) КАК ДолгКлиента,
	СУММА(ВЫБОР
			КОГДА ВзаиморасчетыСКонтрагентамиОстаткиИОбороты.Период = &amp;ТекДатаМинусГод
				ТОГДА ВзаиморасчетыСКонтрагентамиОстаткиИОбороты.СуммаВзаиморасчетовКонечныйОстаток
			ИНАЧЕ ВЫБОР
					КОГДА ВзаиморасчетыСКонтрагентамиОстаткиИОбороты.Период &lt;= ДатыОплатПлан.СделкаДатаОплаты
						ТОГДА ВзаиморасчетыСКонтрагентамиОстаткиИОбороты.СуммаВзаиморасчетовПриход
					ИНАЧЕ 0
				КОНЕЦ
		КОНЕЦ) КАК ОбщийДолгПоЗаказуНаДатуОплаты,
	ВзаиморасчетыСКонтрагентамиОстаткиИОбороты.ДоговорКонтрагента.Владелец
ПОМЕСТИТЬ ДолгиПоЗаказамНеОбработ
ИЗ
	ДатыОплатПлан КАК ДатыОплатПлан
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрНакопления.ВзаиморасчетыСКонтрагентами.ОстаткиИОбороты(&amp;ТекДатаМинус1_5Года, , День, , ДоговорКонтрагента.Владелец = &amp;ТекКонтрагент) КАК ВзаиморасчетыСКонтрагентамиОстаткиИОбороты
		ПО ДатыОплатПлан.Сделка = ВзаиморасчетыСКонтрагентамиОстаткиИОбороты.Сделка

СГРУППИРОВАТЬ ПО
	ДатыОплатПлан.Сделка,
	ДатыОплатПлан.СделкаДатаОплаты,
	ВзаиморасчетыСКонтрагентамиОстаткиИОбороты.ДоговорКонтрагента.Владелец
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ДолгиПоЗаказамНеОбработ.Сделка,
	ДолгиПоЗаказамНеОбработ.СделкаДатаОплаты,
	СУММА(ВзаиморасчетыСКонтрагентамиОстаткиИОбороты.СуммаВзаиморасчетовОборот) КАК СуммаВзаиморасчетовОборот,
	ДолгиПоЗаказамНеОбработ.ДоговорКонтрагентаВладелец,
	МИНИМУМ(ВЫБОР
			КОГДА ВзаиморасчетыСКонтрагентамиОстаткиИОбороты.СуммаВзаиморасчетовКонечныйОстаток &lt;= 0
					И РАЗНОСТЬДАТ(ВзаиморасчетыСКонтрагентамиОстаткиИОбороты.Период, ДолгиПоЗаказамНеОбработ.СделкаДатаОплаты, ДЕНЬ) &lt; 0
				ТОГДА РАЗНОСТЬДАТ(ДолгиПоЗаказамНеОбработ.СделкаДатаОплаты, ВзаиморасчетыСКонтрагентамиОстаткиИОбороты.Период, ДЕНЬ)
			ИНАЧЕ РАЗНОСТЬДАТ(ДолгиПоЗаказамНеОбработ.СделкаДатаОплаты, &amp;ТекДата, ДЕНЬ)
		КОНЕЦ) КАК ДнейПросрочки
ПОМЕСТИТЬ ДнейПДЗНеОбработан
ИЗ
	ДолгиПоЗаказамНеОбработ КАК ДолгиПоЗаказамНеОбработ
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ВзаиморасчетыСКонтрагентами.ОстаткиИОбороты(&amp;ТекДатаМинус1_5Года, , День, , ДоговорКонтрагента.Владелец = &amp;ТекКонтрагент) КАК ВзаиморасчетыСКонтрагентамиОстаткиИОбороты
		ПО ДолгиПоЗаказамНеОбработ.Сделка = ВзаиморасчетыСКонтрагентамиОстаткиИОбороты.Сделка
ГДЕ
	ДолгиПоЗаказамНеОбработ.ДолгКлиента &gt; 0

СГРУППИРОВАТЬ ПО
	ДолгиПоЗаказамНеОбработ.СделкаДатаОплаты,
	ДолгиПоЗаказамНеОбработ.Сделка,
	ДолгиПоЗаказамНеОбработ.ДоговорКонтрагентаВладелец
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ДолгиПоЗаказамНеОбработ.СделкаДатаОплаты) КАК СделкаДатаОплаты,
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ДолгиПоЗаказамНеОбработ.Сделка) КАК Сделка,
	СРЕДНЕЕ(ВЫБОР
			КОГДА ДолгиПоЗаказамНеОбработ.ДолгКлиента &lt;= 0
				ТОГДА 0
			ИНАЧЕ ДолгиПоЗаказамНеОбработ.ДолгКлиента / ДолгиПоЗаказамНеОбработ.ОбщийДолгПоЗаказуНаДатуОплаты * 100
		КОНЕЦ) КАК ПроцентПДЗОтОбщейСуммы,
	СУММА(ДолгиПоЗаказамНеОбработ.ДолгКлиента) КАК ДолгКлиента,
	СУММА(ДолгиПоЗаказамНеОбработ.ОбщийДолгПоЗаказуНаДатуОплаты) КАК ОбщийДолгПоЗаказуНаДатуОплаты,
	ДолгиПоЗаказамНеОбработ.ДоговорКонтрагентаВладелец
ПОМЕСТИТЬ ПроцентПДЗОтОбщейСуммыДолга
ИЗ
	ДолгиПоЗаказамНеОбработ КАК ДолгиПоЗаказамНеОбработ

СГРУППИРОВАТЬ ПО
	ДолгиПоЗаказамНеОбработ.ДоговорКонтрагентаВладелец
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ДолгиПоЗаказамНеОбработ.Сделка) КАК Сделка,
	СРЕДНЕЕ(ДолгиПоЗаказамНеОбработ.ДолгКлиента) КАК ДолгКлиента,
	ДолгиПоЗаказамНеОбработ.ДоговорКонтрагентаВладелец
ПОМЕСТИТЬ СредняяСуммаПДЗ
ИЗ
	ДолгиПоЗаказамНеОбработ КАК ДолгиПоЗаказамНеОбработ
ГДЕ
	ДолгиПоЗаказамНеОбработ.ДолгКлиента &gt; 0

СГРУППИРОВАТЬ ПО
	ДолгиПоЗаказамНеОбработ.ДоговорКонтрагентаВладелец
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ДнейПДЗНеОбработан.Сделка) КАК Сделка,
	ДнейПДЗНеОбработан.ДоговорКонтрагентаВладелец,
	СРЕДНЕЕ(ДнейПДЗНеОбработан.ДнейПросрочки) КАК ДнейПросрочки
ПОМЕСТИТЬ ДнейПДЗ
ИЗ
	ДнейПДЗНеОбработан КАК ДнейПДЗНеОбработан

СГРУППИРОВАТЬ ПО
	ДнейПДЗНеОбработан.ДоговорКонтрагентаВладелец
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЗначенияСвойствОбъектов.Значение,
	ЗначенияСвойствОбъектов.Объект
ПОМЕСТИТЬ Важность
ИЗ
	РегистрСведений.ЗначенияСвойствОбъектов КАК ЗначенияСвойствОбъектов
ГДЕ
	ЗначенияСвойствОбъектов.Свойство.Код = "90184"
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Контрагенты.Ссылка КАК Покупатель,
	ВзаиморасчетыСКонтрагентамиОстатки.ДоговорКонтрагента,
	ВзаиморасчетыСКонтрагентамиОстатки.Сделка,
	Контрагенты.КатегорияПокупателя КАК НаправлениеПродаж,
	Важность.Значение КАК Важность,
	ВложенныйЗапрос.Поле2 КАК Регион,
	СУММА(ВзаиморасчетыСКонтрагентамиОстатки.СуммаВзаиморасчетовОстаток) КАК ДебиторскаяЗадолженность,
	СУММА(ВЫБОР
			КОГДА ВзаиморасчетыСКонтрагентамиОстатки.Сделка ССЫЛКА Документ.ЗаказПокупателя
					И ВзаиморасчетыСКонтрагентамиОстатки.СуммаВзаиморасчетовОстаток &gt; 0
				ТОГДА ВЫБОР
						КОГДА РАЗНОСТЬДАТ(ВзаиморасчетыСКонтрагентамиОстатки.Сделка.ДатаОплаты, &amp;ТекДата, ДЕНЬ) &gt; 0
							ТОГДА ВЫБОР
									КОГДА ВзаиморасчетыСКонтрагентамиОстатки.Сделка.ДатаОплаты &lt; &amp;ТекДата
										ТОГДА ВзаиморасчетыСКонтрагентамиОстатки.СуммаВзаиморасчетовОстаток
									ИНАЧЕ 0
								КОНЕЦ
						ИНАЧЕ 0
					КОНЕЦ
			ИНАЧЕ 0
		КОНЕЦ) КАК ПросроченнаяДебиторскаяЗадолженность,
	Контрагенты.ДопустимаяСуммаЗадолженности,
	ВзаиморасчетыСКонтрагентамиОстатки.ДоговорКонтрагента.ОтветственноеЛицо,
	ПроцентПДЗОтОбщейСуммыДолга.ПроцентПДЗОтОбщейСуммы КАК СреднееПроцентПДЗОтОбщейСуммы,
	СредняяСуммаПДЗ.ДолгКлиента КАК СреднееДолгКлиента,
	ДнейПДЗ.ДнейПросрочки КАК СреднееДнейПросрочки
{ВЫБРАТЬ
	Регион,
	Важность.*,
	Важность.*,
	Регион,
	ДебиторскаяЗадолженность,
	ПросроченнаяДебиторскаяЗадолженность}
ИЗ
	Справочник.Контрагенты КАК Контрагенты
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ВзаиморасчетыСКонтрагентами.Остатки(&amp;ТекДата, ДоговорКонтрагента.Владелец = &amp;ТекКонтрагент) КАК ВзаиморасчетыСКонтрагентамиОстатки
			ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрНакопления.ВзаиморасчетыСКонтрагентами.Остатки КАК ВзаиморасчетыСКонтрагентамиОстатки1
			ПО ВзаиморасчетыСКонтрагентамиОстатки.ДоговорКонтрагента = ВзаиморасчетыСКонтрагентамиОстатки1.ДоговорКонтрагента
				И (ВзаиморасчетыСКонтрагентамиОстатки1.СуммаВзаиморасчетовОстаток &gt; 0)
		ПО Контрагенты.Ссылка = ВзаиморасчетыСКонтрагентамиОстатки.ДоговорКонтрагента.Владелец
		ЛЕВОЕ СОЕДИНЕНИЕ Важность КАК Важность
		ПО Контрагенты.Ссылка = Важность.Объект
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			КонтактнаяИнформация.Объект КАК Объект,
			КонтактнаяИнформация.Поле2 КАК Поле2
		ИЗ
			РегистрСведений.КонтактнаяИнформация КАК КонтактнаяИнформация
		ГДЕ
			КонтактнаяИнформация.Тип = ЗНАЧЕНИЕ(Перечисление.ТипыКонтактнойИнформации.Адрес)
			И КонтактнаяИнформация.Вид = ЗНАЧЕНИЕ(Справочник.ВидыКонтактнойИнформации.ФактАдресКонтрагента)) КАК ВложенныйЗапрос
		ПО Контрагенты.Ссылка = ВложенныйЗапрос.Объект
		ЛЕВОЕ СОЕДИНЕНИЕ ПроцентПДЗОтОбщейСуммыДолга КАК ПроцентПДЗОтОбщейСуммыДолга
		ПО Контрагенты.Ссылка = ПроцентПДЗОтОбщейСуммыДолга.ДоговорКонтрагентаВладелец
		ЛЕВОЕ СОЕДИНЕНИЕ СредняяСуммаПДЗ КАК СредняяСуммаПДЗ
		ПО Контрагенты.Ссылка = СредняяСуммаПДЗ.ДоговорКонтрагентаВладелец
		ЛЕВОЕ СОЕДИНЕНИЕ ДнейПДЗ КАК ДнейПДЗ
		ПО Контрагенты.Ссылка = ДнейПДЗ.ДоговорКонтрагентаВладелец
ГДЕ
	Контрагенты.Ссылка = &amp;ТекКонтрагент
{ГДЕ
	Важность.Значение.* КАК Важность,
	ВложенныйЗапрос.Поле2 КАК Регион,
	(ВЫБОР
			КОГДА ВзаиморасчетыСКонтрагентамиОстатки.СуммаВзаиморасчетовОстаток &gt; 0
				ТОГДА ВзаиморасчетыСКонтрагентамиОстатки.СуммаВзаиморасчетовОстаток
			ИНАЧЕ 0
		КОНЕЦ) КАК Поле1,
	(ВЫБОР
			КОГДА ВзаиморасчетыСКонтрагентамиОстатки.Сделка ССЫЛКА Документ.ЗаказПокупателя
				ТОГДА ВЫБОР
						КОГДА РАЗНОСТЬДАТ(ВзаиморасчетыСКонтрагентамиОстатки.Сделка.ДатаОплаты, &amp;МинДата, ДЕНЬ) &gt; 0
							ТОГДА ВЫБОР
									КОГДА ВзаиморасчетыСКонтрагентамиОстатки.Сделка.ДатаОплаты &lt; &amp;МинДата
										ТОГДА ВзаиморасчетыСКонтрагентамиОстатки.СуммаВзаиморасчетовОстаток
									ИНАЧЕ 0
								КОНЕЦ
						ИНАЧЕ 0
					КОНЕЦ
			ИНАЧЕ 0
		КОНЕЦ) КАК Поле2}

СГРУППИРОВАТЬ ПО
	Важность.Значение,
	ВложенныйЗапрос.Поле2,
	Контрагенты.Ссылка,
	Контрагенты.КатегорияПокупателя,
	ВзаиморасчетыСКонтрагентамиОстатки.ДоговорКонтрагента,
	ВзаиморасчетыСКонтрагентамиОстатки.Сделка,
