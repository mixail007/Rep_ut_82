
Процедура КнопкаВыполнитьНажатие(Кнопка)
	// Вставить содержимое обработчика.
	структуранастроек = новый соответствие;
	СтруктураНастроек.Очистить();
	структураНастроек.Вставить("Организация",ЭлементыФормы.Организация.Значение);
	структураНастроек.Вставить("Склад",ЭлементыФормы.Склад.Значение);
	структураНастроек.Вставить("Валюта",ЭлементыФормы.Валюта.Значение); 
	структураНастроек.Вставить("Контрагент",ЭлементыФормы.Контрагент.Значение); 
	структураНастроек.Вставить("ТипЦен",ЭлементыФормы.ТипЦен.Значение); 
	Сохранитьзначение("СтруктураНастроекЗаявкиПоставщику",СтруктураНастроек);
	Закрыть();
КонецПроцедуры

Процедура ПриОткрытии()
	// Вставить содержимое обработчика.
	если (СтруктураНастроек<>неопределено) тогда 
		Организация 		= СтруктураНастроек.Получить("Организация");
		Склад 				= СтруктураНастроек.Получить("Склад");
		Валюта 				= СтруктураНастроек.Получить("Валюта");
		Контрагент 			= СтруктураНастроек.Получить("Контрагент");
		ТипЦен				= СтруктураНастроек.Получить("ТипЦен");
	иначе 
		Организация 		= Справочники.Организации.НайтиПоНаименованию("ООО ""Формула""");
		Склад 				= Справочники.Склады.НайтиПоНаименованию("Склад магазина");
		Валюта 				= Справочники.Валюты.НайтиПоКоду("810");
		Контрагент 			= Справочники.Контрагенты.НайтиПоНаименованию("Яршинторг ЗАО");		
	конецесли;
КонецПроцедуры

