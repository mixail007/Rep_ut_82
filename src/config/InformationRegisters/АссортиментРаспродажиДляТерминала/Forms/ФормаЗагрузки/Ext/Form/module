
Процедура ОсновныеДействияФормыЗагрузить(Кнопка)
	Значение=ЭлементыФормы.ТабличныйДокумент.Область("R1C1").Текст;
	номерСтроки=1;
	Пока Значение <>"" цикл
	 попытка
		Номенклатура = Справочники.Номенклатура.НайтиПоКоду(Значение);
		Если Номенклатура <> Справочники.Номенклатура.ПустаяСсылка()  тогда
			Запись = РегистрыСведений.АссортиментРаспродажиДляТерминала.СоздатьМенеджерЗаписи();
			Запись.Номенклатура = Номенклатура;
			Запись.Записать(Истина);
		иначе
			Сообщить("По коду в строке "+номерСтроки+" номенклатура не найдена!");
		конецЕсли;
	 исключение	
			Сообщить("По коду в строке "+номерСтроки+" номенклатура не найдена!");
		конецПопытки;
	 номерСтроки=номерСтроки+1; 
	 Значение=ЭлементыФормы.ТабличныйДокумент.Область("R"+Формат(НомерСтроки,"ЧГ=")+"C1").Текст;
	конецЦикла;	
КонецПроцедуры
