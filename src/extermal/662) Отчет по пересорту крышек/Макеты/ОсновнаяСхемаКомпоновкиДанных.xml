			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ
	Б.ЗаказПоставщикуКрышки.Контрагент КАК Поставщик,
	Б.ЗаказПоставщикуКрышки,
	Б.Номенклатура,
	Б.КоличествоЗаказано,
	Б.КоличествоПоступило,
	Б.Разница,
	Б.РазницаПоСумме,
	Б.СуммаЗаказано,
	Б.СуммаПоступило
ИЗ
	(ВЫБРАТЬ
		А.ЗаказПоставщикуКрышки КАК ЗаказПоставщикуКрышки,
		А.Номенклатура КАК Номенклатура,
		А.КоличествоЗаказано КАК КоличествоЗаказано,
		ПоставкиКрышки.КоличествоРасход КАК КоличествоПоступило,
		А.КоличествоЗаказано - ЕСТЬNULL(ПоставкиКрышки.КоличествоРасход, 0) КАК Разница,
		А.СуммаЗаказано - ЕСТЬNULL(ПоставкиКрышки.СуммаВзаиморасчетовРасход, 0) КАК РазницаПоСумме,
		А.СуммаЗаказано КАК СуммаЗаказано,
		ПоставкиКрышки.СуммаВзаиморасчетовРасход КАК СуммаПоступило
	ИЗ
		(ВЫБРАТЬ
			ЗаказыПоставщикамКрышки.ЗаказПоставщикуКрышки КАК ЗаказПоставщикуКрышки,
			ЗаказыПоставщикамКрышки.Номенклатура КАК Номенклатура,
			СУММА(ЗаказыПоставщикамКрышки.КоличествоПриход) КАК КоличествоЗаказано,
			ЗаказыПоставщикамКрышки.СуммаВзаиморасчетовПриход КАК СуммаЗаказано
		ИЗ
			РегистрНакопления.ЗаказыПоставщикамКрышки.Обороты(&amp;НачПериода, &amp;КонПериода, Регистратор, ) КАК ЗаказыПоставщикамКрышки
		ГДЕ
			ЗаказыПоставщикамКрышки.Регистратор ССЫЛКА Документ.ЗаказПоставщикуКрышки
		
		СГРУППИРОВАТЬ ПО
			ЗаказыПоставщикамКрышки.ЗаказПоставщикуКрышки,
			ЗаказыПоставщикамКрышки.Номенклатура,
			ЗаказыПоставщикамКрышки.СуммаВзаиморасчетовПриход) КАК А
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ЗаказыПоставщикамКрышки.Обороты КАК ПоставкиКрышки
			ПО А.Номенклатура = ПоставкиКрышки.Номенклатура
				И А.ЗаказПоставщикуКрышки = ПоставкиКрышки.ЗаказПоставщикуКрышки) КАК Б
ГДЕ
	Б.Разница &lt;&gt; 0

УПОРЯДОЧИТЬ ПО
	Б.ЗаказПоставщикуКрышки.Дата</query>
