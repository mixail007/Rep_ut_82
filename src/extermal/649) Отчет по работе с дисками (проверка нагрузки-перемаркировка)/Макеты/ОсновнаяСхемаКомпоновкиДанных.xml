			<name>НаборДанных4</name>
			<field xsi:type="DataSetFieldField">
				<dataPath>СуммаПоКлиентам</dataPath>
				<field>СуммаПоКлиентам</field>
			</field>
			<field xsi:type="DataSetFieldField">
				<dataPath>КодПоКлиентам</dataPath>
				<field>КодПоКлиентам</field>
			</field>
			<field xsi:type="DataSetFieldField">
