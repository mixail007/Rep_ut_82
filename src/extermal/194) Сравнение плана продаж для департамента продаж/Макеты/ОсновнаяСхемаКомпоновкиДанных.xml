			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ
	ХозрасчетныйОборотыДтКт.Менеджер,
	ХозрасчетныйОборотыДтКт.СубконтоДт1,
	ХозрасчетныйОборотыДтКт.СубконтоДт2,
	СУММА(ХозрасчетныйОборотыДтКт.СуммаОборот) КАК СуммаОборот,
	СУММА(ВЫБОР
			КОГДА ХозрасчетныйОборотыДтКт.СчетКт = &amp;счет42
				ТОГДА ХозрасчетныйОборотыДтКт.СуммаОборот
			ИНАЧЕ 0
		КОНЕЦ) КАК СуммаНаценка,
	СУММА(ХозрасчетныйОборотыДтКт.КоличествоОборотКт) КАК КоличествоОборотДт
ПОМЕСТИТЬ ПланРТГ
ИЗ
	РегистрБухгалтерии.Хозрасчетный.ОборотыДтКт(, , , СчетДт = &amp;Сч, , СчетКт В (&amp;Счета), , ПериодПланирования = &amp;ПериодПланирования) КАК ХозрасчетныйОборотыДтКт

СГРУППИРОВАТЬ ПО
	ХозрасчетныйОборотыДтКт.СубконтоДт2,
	ХозрасчетныйОборотыДтКт.Менеджер,
	ХозрасчетныйОборотыДтКт.СубконтоДт1
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ПланРТГ.Менеджер,
	ПланРТГ.СубконтоДт1,
	СУММА(ПланРТГ.СуммаОборот) КАК СуммаОборот,
	СУММА(ПланРТГ.СуммаНаценка) КАК СуммаНаценка,
	СУММА(ПланРТГ.КоличествоОборотДт) КАК КоличествоОборотДт
ПОМЕСТИТЬ ПланРТГпоНГ
ИЗ
	ПланРТГ КАК ПланРТГ

СГРУППИРОВАТЬ ПО
	ПланРТГ.СубконтоДт1,
	ПланРТГ.Менеджер
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ТоварныеГруппыДляПланированияСостав.НоменклатурнаяГруппа,
	ТоварныеГруппыДляПланированияСостав.Ссылка КАК ТоварнаяГруппа
ПОМЕСТИТЬ ТоварныеГруппы
ИЗ
	Справочник.ТоварныеГруппыДляПланирования.Состав КАК ТоварныеГруппыДляПланированияСостав
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ПолугодовойПланДепертаментаПродаж.НоменклатурнаяГруппа,
	ПолугодовойПланДепертаментаПродаж.НаправлениеПродаж.Подразделение,
	СУММА(ПолугодовойПланДепертаментаПродаж.КоличествоПлан) КАК КоличествоПлан,
	СУММА(ПолугодовойПланДепертаментаПродаж.СуммаПлан) КАК СуммаПлан,
	СУММА(ПолугодовойПланДепертаментаПродаж.Количество) КАК Количество,
	СУММА(ПолугодовойПланДепертаментаПродаж.СуммаНаценкиПлан) КАК СуммаНаценкиПлан,
	ПолугодовойПланДепертаментаПродаж.НоменклатурнаяГруппа.Родитель,
	ПолугодовойПланДепертаментаПродаж.НоменклатурнаяГруппа.Ответственный,
	ПолугодовойПланДепертаментаПродаж.НаправлениеПродаж,
	ТоварныеГруппы.ТоварнаяГруппа
ПОМЕСТИТЬ ПланПодразделений
ИЗ
	РегистрСведений.ПолугодовойПланДепертаментаПродаж КАК ПолугодовойПланДепертаментаПродаж
		ЛЕВОЕ СОЕДИНЕНИЕ ТоварныеГруппы КАК ТоварныеГруппы
		ПО ПолугодовойПланДепертаментаПродаж.НоменклатурнаяГруппа = ТоварныеГруппы.НоменклатурнаяГруппа
ГДЕ
	ПолугодовойПланДепертаментаПродаж.ПериодПланирования = &amp;ПериодПланирования

СГРУППИРОВАТЬ ПО
	ПолугодовойПланДепертаментаПродаж.НоменклатурнаяГруппа,
	ПолугодовойПланДепертаментаПродаж.НаправлениеПродаж.Подразделение,
	ПолугодовойПланДепертаментаПродаж.НоменклатурнаяГруппа.Родитель,
	ПолугодовойПланДепертаментаПродаж.НоменклатурнаяГруппа.Ответственный,
	ПолугодовойПланДепертаментаПродаж.НаправлениеПродаж,
	ТоварныеГруппы.ТоварнаяГруппа
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ПланРТГ.Менеджер КАК Менеджер,
	ПланРТГ.СубконтоДт1 КАК Разрез1,
	ПланРТГ.СубконтоДт2 КАК Производитель,
	ПланРТГ.КоличествоОборотДт КАК Количество,
	ПланРТГ.СуммаОборот КАК Сумма,
	ПланРТГ.СуммаНаценка,
	План.НаправлениеПродажПодразделение КАК Подразделение,
	План.КоличествоПлан * (ПланРТГ.КоличествоОборотДт / ПланРТГпоНГ.КоличествоОборотДт) КАК ПланПодразделения,
	План.СуммаПлан * (ПланРТГ.КоличествоОборотДт / ПланРТГпоНГ.КоличествоОборотДт) КАК СуммаПодразделения,
	ВЫБОР
		КОГДА План.СуммаПлан - План.СуммаНаценкиПлан = 0
			ТОГДА 0
		ИНАЧЕ План.СуммаНаценкиПлан / (План.СуммаПлан - План.СуммаНаценкиПлан) * 100
	КОНЕЦ КАК ПроцентНаценкиПодразделения,
	План.СуммаНаценкиПлан * (ПланРТГ.КоличествоОборотДт / ПланРТГпоНГ.КоличествоОборотДт) КАК СуммаНаценкаПодразделения,
	План.НаправлениеПродаж,
	ВЫБОР
		КОГДА План.ТоварнаяГруппа В ИЕРАРХИИ (&amp;Аксы)
			ТОГДА &amp;Аксы
		ИНАЧЕ План.ТоварнаяГруппа
	КОНЕЦ КАК ТоварнаяГруппа
ИЗ
	ПланРТГ КАК ПланРТГ
		ПОЛНОЕ СОЕДИНЕНИЕ ПланПодразделений КАК План
		ПО ПланРТГ.Менеджер = План.НоменклатурнаяГруппаОтветственный
			И (ВЫБОР
				КОГДА ПланРТГ.СубконтоДт1.ЭтоГруппа
					ТОГДА ПланРТГ.СубконтоДт1 = План.НоменклатурнаяГруппаРодитель
				ИНАЧЕ ПланРТГ.СубконтоДт1 = План.НоменклатурнаяГруппа
			КОНЕЦ)
		ЛЕВОЕ СОЕДИНЕНИЕ ПланРТГпоНГ КАК ПланРТГпоНГ
		ПО ПланРТГ.Менеджер = ПланРТГпоНГ.Менеджер
			И ПланРТГ.СубконтоДт1 = ПланРТГпоНГ.СубконтоДт1

УПОРЯДОЧИТЬ ПО
	Менеджер,
	Разрез1,
	Производитель,
	Подразделение</query>
		</dataSet>
		<totalField>
			<dataPath>Количество</dataPath>
			<expression>Максимум(Количество)</expression>
		</totalField>
		<totalField>
			<dataPath>ПланПодразделения</dataPath>
			<expression>Сумма(ПланПодразделения)</expression>
		</totalField>
		<totalField>
			<dataPath>СуммаНаценка</dataPath>
			<expression>Максимум(СуммаНаценка)</expression>
		</totalField>
		<totalField>
			<dataPath>СуммаПодразделения</dataPath>
			<expression>Сумма(СуммаПодразделения)</expression>
		</totalField>
		<totalField>
			<dataPath>Сумма</dataPath>
			<expression>Максимум(Сумма)</expression>
		</totalField>
		<totalField>
			<dataPath>Сумма</dataPath>
			<expression>0</expression>
			<group>НаправлениеПродаж</group>
			<group>НаправлениеПродаж Иерархия</group>
			<group>Подразделение</group>
		</totalField>
		<totalField>
			<dataPath>Количество</dataPath>
			<expression>0</expression>
			<group>НаправлениеПродаж</group>
			<group>НаправлениеПродаж Иерархия</group>
			<group>Подразделение</group>
		</totalField>
		<totalField>
			<dataPath>СуммаНаценка</dataPath>
			<expression>0</expression>
			<group>НаправлениеПродаж</group>
			<group>НаправлениеПродаж Иерархия</group>
			<group>Подразделение</group>
		</totalField>
		<totalField>
			<dataPath>Количество</dataPath>
			<expression>Сумма(ВычислитьВыражениеСГруппировкойМассив("максимум(Количество)","Разрез1,Производитель"))</expression>
			<group>Менеджер</group>
			<group>Разрез1</group>
			<group>ТоварнаяГруппа</group>
			<group>ТоварнаяГруппа Иерархия</group>
			<group>ОбщийИтог</group>
		</totalField>
		<totalField>
			<dataPath>Сумма</dataPath>
			<expression>Сумма(ВычислитьВыражениеСГруппировкойМассив("Максимум(Сумма)","Разрез1,Производитель"))</expression>
			<group>Менеджер</group>
			<group>Разрез1</group>
			<group>ТоварнаяГруппа</group>
			<group>ТоварнаяГруппа Иерархия</group>
			<group>ОбщийИтог</group>
		</totalField>
		<totalField>
			<dataPath>СуммаНаценка</dataPath>
			<expression>Сумма(ВычислитьВыражениеСГруппировкойМассив("Максимум(СуммаНаценка)","Разрез1,Производитель"))</expression>
			<group>Менеджер</group>
			<group>Разрез1</group>
			<group>ТоварнаяГруппа</group>
			<group>ТоварнаяГруппа Иерархия</group>
			<group>ОбщийИтог</group>
		</totalField>
		<totalField>
			<dataPath>СуммаНаценкаПодразделения</dataPath>
			<expression>Сумма(СуммаНаценкаПодразделения)</expression>
		</totalField>
		<parameter>
			<name>НачалоПериода</name>
			<title xmlns:d4p1="http://v8.1c.ru/8.1/data/core" xsi:type="d4p1:LocalStringType">
				<d4p1:item>
					<d4p1:lang>ru</d4p1:lang>
					<d4p1:content>Начало периода</d4p1:content>
				</d4p1:item>
			</title>
			<valueType>
				<Type xmlns="http://v8.1c.ru/8.1/data/core">xs:dateTime</Type>
				<DateQualifiers xmlns="http://v8.1c.ru/8.1/data/core">
					<DateFractions>DateTime</DateFractions>
				</DateQualifiers>
			</valueType>
