			<expression>СУММА( СтоимостьОборот)/Сумма(КоличествоПродаж)</expression>
		</totalField>
		<totalField>
			<dataPath>КоличествоПродаж</dataPath>
			<expression>Сумма(КоличествоПродаж)</expression>
		</totalField>
