	ЕСТЬNULL(Бюджет.СуммаБюджет, 0) КАК СуммаБюджет,
	0 КАК Отклонение,
	ЕСТЬNULL(Бюджет.СуммаБюджет, 0) КАК СуммаБюджет1,
	"Бюджет" КАК Бюджет
ИЗ
	Бюджет КАК Бюджет
		ПОЛНОЕ СОЕДИНЕНИЕ ФАКТ КАК ФАКТ
		ПО Бюджет.Подразделение = ФАКТ.Подразделение
			И Бюджет.СтатьяЗатрат = ФАКТ.СтатьяЗатрат</query>
		</dataSet>
		<calculatedField>
			<dataPath>откл</dataPath>
			<expression>СуммаБюджет-СуммаОборот</expression>
			<title xmlns:d4p1="http://v8.1c.ru/8.1/data/core" xsi:type="d4p1:LocalStringType">
