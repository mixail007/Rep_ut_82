			<expression>Сумма(СуммаМатериалов)</expression>
		</totalField>
		<totalField>
			<dataPath>ЦенаСебестоимость</dataPath>
			<expression>Среднее(ЦенаСебестоимость)</expression>
		</totalField>
		<totalField>
			<dataPath>КоличествоПродано</dataPath>
			<expression>Сумма(КоличествоПродано)</expression>
		</totalField>
		<totalField>
			<dataPath>СтоимостьПродано</dataPath>
			<expression>Сумма(СтоимостьПродано)</expression>
		</totalField>
		<totalField>
			<dataPath>ВаловаяПрибыль</dataPath>
			<expression>Сумма(ВаловаяПрибыль)</expression>
		</totalField>
		<totalField>
			<dataPath>ПроцентНаценки</dataPath>
			<expression>100*Сумма(ВаловаяПрибыль)/Сумма(Сумма) </expression>
		</totalField>
