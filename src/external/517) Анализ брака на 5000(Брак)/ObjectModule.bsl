
Процедура ПриКомпоновкеРезультата(ДокументРезультат, ДанныеРасшифровки, СтандартнаяОбработка)
	Параметр = КомпоновщикНастроек.Настройки.ПараметрыДанных.НайтиЗначениеПараметра(Новый ПараметрКомпоновкиДанных("началоПериода1"));
    Если Параметр <> Неопределено Тогда
        Параметр.Значение = Дата1;
        Параметр.Использование = Истина;
	КонецЕсли;
	
	Параметр = КомпоновщикНастроек.Настройки.ПараметрыДанных.НайтиЗначениеПараметра(Новый ПараметрКомпоновкиДанных("КонецПериода1"));
	Если Параметр <> Неопределено Тогда
		Параметр.Значение = Дата2;
		Параметр.Использование = Истина;
	КонецЕсли;
	
	Параметр = КомпоновщикНастроек.Настройки.ПараметрыДанных.НайтиЗначениеПараметра(Новый ПараметрКомпоновкиДанных("ДатаТ"));
	Если Параметр <> Неопределено Тогда
		Параметр.Значение = Дата("20160101000000");
		Параметр.Использование = Истина;
	КонецЕсли;
	
	Параметр = КомпоновщикНастроек.Настройки.ПараметрыДанных.НайтиЗначениеПараметра(Новый ПараметрКомпоновкиДанных("брак"));
	Если Параметр <> Неопределено Тогда
		Параметр.Значение = Справочники.Склады.НайтиПоКоду("00925");
		Параметр.Использование = Истина;
	КонецЕсли;

