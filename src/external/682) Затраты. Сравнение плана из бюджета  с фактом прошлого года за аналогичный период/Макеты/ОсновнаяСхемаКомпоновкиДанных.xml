;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ПланированиеЗатратПодразделения.Подразделение,
	ПланированиеЗатратПодразделения.ПериодПланирования,
	ПланированиеЗатратПодразделения.СтатьяЗатрат,
	СУММА(ПланированиеЗатратПодразделения.СуммаЗатрат) КАК СуммаЗатрат
ПОМЕСТИТЬ ДетальныеЗаписи
ИЗ
	РегистрСведений.ПланированиеЗатратПодразделения КАК ПланированиеЗатратПодразделения
ГДЕ
	ПланированиеЗатратПодразделения.ПериодПланирования = &amp;ПериодПланирования

СГРУППИРОВАТЬ ПО
	ПланированиеЗатратПодразделения.ПериодПланирования,
	ПланированиеЗатратПодразделения.Подразделение,
	ПланированиеЗатратПодразделения.СтатьяЗатрат
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЕСТЬNULL(ЕСТЬNULL(ЗатратыБюджет.Субконто1, ЗатратыПрошлыйГод.Подразделение), ДетальныеЗаписи1.Подразделение) КАК Подразделение,
	ЕСТЬNULL(ЕСТЬNULL(ЗатратыБюджет.Субконто2, ЗатратыПрошлыйГод.СтатьяЗатрат), ДетальныеЗаписи1.СтатьяЗатрат) КАК СтатьяЗатрат,
	ЕСТЬNULL(ЗатратыБюджет.СуммаОборот, 0) КАК СуммаБюджет,
	ЕСТЬNULL(ЗатратыПрошлыйГод.СуммаОборот, 0) КАК СуммаПрошлыйГод,
	ЕСТЬNULL(ЗатратыБюджет.СуммаОборот, 0) - ЕСТЬNULL(ЗатратыПрошлыйГод.СуммаОборот, 0) КАК АбсолютноеОтклонение,
	ЕСТЬNULL(ДетальныеЗаписи1.СуммаЗатрат, 0) КАК СуммаЗатрат,
	ЕСТЬNULL(ЕСТЬNULL(ЗатратыБюджет.Субконто2.Ответственный, ЗатратыПрошлыйГод.СтатьяЗатрат.Ответственный), ДетальныеЗаписи1.СтатьяЗатрат.Ответственный) КАК Ответственный
ПОМЕСТИТЬ ВТ
ИЗ
	РегистрБухгалтерии.Хозрасчетный.Обороты(, , , Счет В ИЕРАРХИИ (&amp;СчетЗатраты), , ПериодПланирования = &amp;ПериодПланирования, , ) КАК ЗатратыБюджет
		ПОЛНОЕ СОЕДИНЕНИЕ РегистрНакопления.Затраты.Обороты(&amp;НачФакт, &amp;КонФакт, Авто, ) КАК ЗатратыПрошлыйГод
		ПО ЗатратыБюджет.Субконто1 = ЗатратыПрошлыйГод.Подразделение
			И ЗатратыБюджет.Субконто2 = ЗатратыПрошлыйГод.СтатьяЗатрат
		ПОЛНОЕ СОЕДИНЕНИЕ ДетальныеЗаписи1 КАК ДетальныеЗаписи1
		ПО ЗатратыБюджет.Субконто2 = ДетальныеЗаписи1.СтатьяЗатрат
			И ЗатратыБюджет.Субконто1 = ДетальныеЗаписи1.Подразделение
;

