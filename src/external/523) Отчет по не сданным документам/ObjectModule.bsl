
Процедура ПриКомпоновкеРезультата(ДокументРезультат, ДанныеРасшифровки, СтандартнаяОбработка)
	Параметр = КомпоновщикНастроек.Настройки.ПараметрыДанных.НайтиЗначениеПараметра(Новый ПараметрКомпоновкиДанных("Дата1"));
    Если Параметр <> Неопределено Тогда
        Параметр.Значение = ДобавитьМесяц(ТекущаяДата(),-1);
        Параметр.Использование = Истина;
	КонецЕсли;
	
	Параметр = КомпоновщикНастроек.Настройки.ПараметрыДанных.НайтиЗначениеПараметра(Новый ПараметрКомпоновкиДанных("Дата2"));
	Если Параметр <> Неопределено Тогда
		Параметр.Значение = КонецДня(ТекущаяДата());
		Параметр.Использование = Истина;
	КонецЕсли;
	
	Параметр = КомпоновщикНастроек.Настройки.ПараметрыДанных.НайтиЗначениеПараметра(Новый ПараметрКомпоновкиДанных("ТекущаяДата"));
	Если Параметр <> Неопределено Тогда
		Параметр.Значение = ТекущаяДата();
		Параметр.Использование = Истина;
	КонецЕсли;

КонецПроцедуры
