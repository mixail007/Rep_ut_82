			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ
	ХозрасчетныйОбороты.ПериодПланирования,
	ХозрасчетныйОбороты.Менеджер,
	ХозрасчетныйОбороты.Субконто1,
	ХозрасчетныйОбороты.Субконто2
ПОМЕСТИТЬ База
ИЗ
	РегистрБухгалтерии.Хозрасчетный.Обороты(, , Авто, Счет В (&amp;Список), , ПериодПланирования = &amp;Периодпланирования, , ) КАК ХозрасчетныйОбороты
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	База.Менеджер,
	База.Субконто1 КАК СубконтоКт1,
	База.Субконто2 КАК СубконтоКт2,
	ХозрасчетныйОборотыДтКт.СубконтоДт3,
	ХозрасчетныйОборотыДтКт.ПериодСекунда КАК Период,
	ХозрасчетныйОборотыДтКт.СуммаОборот КАК Сумма
ПОМЕСТИТЬ ПриходДенег
ИЗ
	База КАК База
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрБухгалтерии.Хозрасчетный.ОборотыДтКт(
				,
				,
				Авто,
				СчетДт = &amp;ДенежныеСредства,
				,
				СчетКт = &amp;РасчетыСПокупателями,
				,
				ПериодПланирования = &amp;ПериодПланирования
					И СубконтоДт3 &lt;&gt; &amp;СДебиторки) КАК ХозрасчетныйОборотыДтКт
		ПО База.Менеджер = ХозрасчетныйОборотыДтКт.Менеджер
			И База.Субконто1 = ХозрасчетныйОборотыДтКт.СубконтоКт1
			И База.Субконто2 = ХозрасчетныйОборотыДтКт.СубконтоКт2
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ХозрасчетныйОборотыДтКт.Менеджер,
	ХозрасчетныйОборотыДтКт.СубконтоДт1,
	ХозрасчетныйОборотыДтКт.СубконтоДт2,
	ХозрасчетныйОборотыДтКт.СуммаОборот КАК Наценка,
	ХозрасчетныйОборотыДтКт.ПериодСекунда КАК Период
ПОМЕСТИТЬ начальныеПоказатели
ИЗ
	РегистрБухгалтерии.Хозрасчетный.ОборотыДтКт(, , Авто, СчетДт = &amp;РасчетыСПокупателями, , СчетКт = &amp;Наценка, , ПериодПланирования = &amp;ПериодПланирования) КАК ХозрасчетныйОборотыДтКт
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ХозрасчетныйОборотыДтКт.Менеджер,
	ХозрасчетныйОборотыДтКт.СубконтоДт1,
	ХозрасчетныйОборотыДтКт.СубконтоДт2,
	ХозрасчетныйОборотыДтКт.КоличествоОборотКт КАК Количество,
	ХозрасчетныйОборотыДтКт.СуммаОборот + ЕСТЬNULL(ХозрасчетныйОборотыДтКт1.СуммаОборот, 0) КАК Сумма,
	ХозрасчетныйОборотыДтКт.ПериодСекунда КАК Период,
	ХозрасчетныйОборотыДтКт1.СуммаОборот КАК Наценка
ПОМЕСТИТЬ Продажи
ИЗ
	РегистрБухгалтерии.Хозрасчетный.ОборотыДтКт(, , Авто, СчетДт = &amp;РасчетыСПокупателями, , СчетКт = &amp;ТоварыНаСкладах, , ПериодПланирования = &amp;ПериодПланирования) КАК ХозрасчетныйОборотыДтКт
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрБухгалтерии.Хозрасчетный.ОборотыДтКт(, , Авто, СчетДт = &amp;РасчетыСПокупателями, , СчетКт = &amp;Наценка, , ПериодПланирования = &amp;ПериодПланирования) КАК ХозрасчетныйОборотыДтКт1
		ПО ХозрасчетныйОборотыДтКт.Менеджер = ХозрасчетныйОборотыДтКт1.Менеджер
			И ХозрасчетныйОборотыДтКт.СубконтоДт1 = ХозрасчетныйОборотыДтКт1.СубконтоДт1
			И ХозрасчетныйОборотыДтКт.СубконтоДт2 = ХозрасчетныйОборотыДтКт1.СубконтоДт2
			И ХозрасчетныйОборотыДтКт.ПериодСекунда = ХозрасчетныйОборотыДтКт1.ПериодСекунда
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Продажи.Менеджер,
	Продажи.СубконтоДт1,
	Продажи.СубконтоДт2,
	Продажи.Период,
	СРЕДНЕЕ((Продажи.Сумма / (Продажи.Сумма - начальныеПоказатели.Наценка) - 1) * 100) КАК ПроцентНаценки
ПОМЕСТИТЬ ПроцентНаценки
ИЗ
	Продажи КАК Продажи
		ЛЕВОЕ СОЕДИНЕНИЕ начальныеПоказатели КАК начальныеПоказатели
		ПО Продажи.Менеджер = начальныеПоказатели.Менеджер
			И Продажи.СубконтоДт1 = начальныеПоказатели.СубконтоДт1
			И Продажи.СубконтоДт2 = начальныеПоказатели.СубконтоДт2
			И Продажи.Период = начальныеПоказатели.Период

СГРУППИРОВАТЬ ПО
	Продажи.Менеджер,
	Продажи.СубконтоДт1,
	Продажи.СубконтоДт2,
	Продажи.Период
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Продажи.Менеджер,
	Продажи.СубконтоДт1,
	Продажи.СубконтоДт2,
	Продажи.Количество,
	Продажи.Сумма,
	Продажи.Период,
	NULL КАК ПроцентНаценки,
	Продажи.Наценка
ПОМЕСТИТЬ Итого
ИЗ
	Продажи КАК Продажи

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ПроцентНаценки1.Менеджер,
	ПроцентНаценки1.СубконтоДт1,
	ПроцентНаценки1.СубконтоДт2,
	NULL,
	NULL,
	ПроцентНаценки1.Период,
	ПроцентНаценки1.ПроцентНаценки,
	NULL
ИЗ
	ПроцентНаценки КАК ПроцентНаценки1
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	База.Менеджер,
	База.Субконто1 КАК СубконтоДт1,
	База.Субконто2 КАК СубконтоДт2,
	СУММА(Итого.Количество) КАК Количество,
	СУММА(Итого.Сумма) КАК Сумма,
	Итого.Период,
	СУММА(Итого.ПроцентНаценки) КАК ПроцентНаценки,
	СУММА(Итого.Наценка) КАК Наценка
ПОМЕСТИТЬ Свод
ИЗ
	База КАК База
		ЛЕВОЕ СОЕДИНЕНИЕ Итого КАК Итого
		ПО База.Менеджер = Итого.Менеджер
			И База.Субконто1 = Итого.СубконтоДт1
			И База.Субконто2 = Итого.СубконтоДт2

СГРУППИРОВАТЬ ПО
	База.Менеджер,
	База.Субконто1,
	База.Субконто2,
	Итого.Период
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЕСТЬNULL(Свод.Менеджер, ПриходДенег.Менеджер) КАК Менеджер,
	ЕСТЬNULL(Свод.СубконтоДт1, ПриходДенег.СубконтоКт1) КАК СубконтоДт1,
	ЕСТЬNULL(Свод.СубконтоДт2, ПриходДенег.СубконтоКт2) КАК СубконтоДт2,
	ЕСТЬNULL(Свод.Период, ПриходДенег.Период) КАК Период,
	Свод.ПроцентНаценки,
	Свод.Наценка,
	ПриходДенег.Сумма КАК СуммаПоступления,
	ПриходДенег.СубконтоДт3,
	ВЫБОР
		КОГДА ПриходДенег.СубконтоДт3 = ЗНАЧЕНИЕ(Справочник.статьиЗатрат.СДебиторки)
			ТОГДА 0
		ИНАЧЕ Свод.Сумма
	КОНЕЦ КАК Сумма,
	ВЫБОР
		КОГДА ПриходДенег.СубконтоДт3 = ЗНАЧЕНИЕ(Справочник.статьиЗатрат.СДебиторки)
			ТОГДА 0
		ИНАЧЕ Свод.Количество
	КОНЕЦ КАК Количество
ИЗ
	Свод КАК Свод
		ПОЛНОЕ СОЕДИНЕНИЕ ПриходДенег КАК ПриходДенег
		ПО Свод.Менеджер = ПриходДенег.Менеджер
			И Свод.СубконтоДт1 = ПриходДенег.СубконтоКт1
			И Свод.СубконтоДт2 = ПриходДенег.СубконтоКт2
			И Свод.Период = ПриходДенег.Период</query>
		</dataSet>
		<totalField>
			<dataPath>ПроцентНаценки</dataPath>
			<expression>Выбор когда (Сумма(Сумма)-Сумма(Наценка))=0 тогда 0 иначе Сумма(Наценка)/(Сумма(Сумма)-Сумма(Наценка))*100 конец</expression>
		</totalField>
		<totalField>
			<dataPath>Наценка</dataPath>
			<expression>Сумма(Наценка)</expression>
		</totalField>
		<totalField>
			<dataPath>СуммаПоступления</dataPath>
			<expression>Сумма(СуммаПоступления)</expression>
		</totalField>
		<totalField>
			<dataPath>Сумма</dataPath>
			<expression>Сумма(Сумма)</expression>
		</totalField>
		<totalField>
			<dataPath>Количество</dataPath>
			<expression>Сумма(Количество)</expression>
		</totalField>
		<parameter>
			<name>НачалоПериода</name>
			<title xmlns:d4p1="http://v8.1c.ru/8.1/data/core" xsi:type="d4p1:LocalStringType">
				<d4p1:item>
					<d4p1:lang>ru</d4p1:lang>
					<d4p1:content>Начало периода</d4p1:content>
				</d4p1:item>
			</title>
			<valueType>
				<Type xmlns="http://v8.1c.ru/8.1/data/core">xs:dateTime</Type>
				<DateQualifiers xmlns="http://v8.1c.ru/8.1/data/core">
					<DateFractions>DateTime</DateFractions>
				</DateQualifiers>
			</valueType>
			<value xsi:type="xs:dateTime">0001-01-01T00:00:00</value>
			<useRestriction>false</useRestriction>
		</parameter>
		<parameter>
			<name>КонецПериода</name>
			<title xmlns:d4p1="http://v8.1c.ru/8.1/data/core" xsi:type="d4p1:LocalStringType">
				<d4p1:item>
					<d4p1:lang>ru</d4p1:lang>
					<d4p1:content>Конец периода</d4p1:content>
				</d4p1:item>
			</title>
			<valueType>
				<Type xmlns="http://v8.1c.ru/8.1/data/core">xs:dateTime</Type>
				<DateQualifiers xmlns="http://v8.1c.ru/8.1/data/core">
					<DateFractions>DateTime</DateFractions>
				</DateQualifiers>
			</valueType>
			<value xsi:type="xs:dateTime">0001-01-01T00:00:00</value>
			<useRestriction>false</useRestriction>
		</parameter>
		<parameter>
			<name>ПериодПланирования</name>
			<title xmlns:d4p1="http://v8.1c.ru/8.1/data/core" xsi:type="d4p1:LocalStringType">
				<d4p1:item>
					<d4p1:lang>ru</d4p1:lang>
					<d4p1:content>Период планирования</d4p1:content>
				</d4p1:item>
			</title>
			<valueType>
				<Type xmlns="http://v8.1c.ru/8.1/data/core" xmlns:d5p1="http://v8.1c.ru/8.1/data/enterprise/current-config">d5p1:CatalogRef.ПериодыПланирования</Type>
			</valueType>
			<value xsi:nil="true"/>
			<useRestriction>true</useRestriction>
		</parameter>
