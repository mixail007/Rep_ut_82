			Курс = Запрос.Параметры.Курс;
            Кратность = Запрос.Параметры.Кратность;
			Для Каждого СтрокаСчетаФактуры Из СтрокиСчетаФактуры Цикл
				СтрокаСчетаФактуры.Сумма = ?(Кратность = 0, 0, окр(СтрокаСчетаФактуры.Сумма * Курс / Кратность,2));
                СтрокаСчетаФактуры.СуммаНДС = ?(Кратность = 0, 0, окр(СтрокаСчетаФактуры.СуммаНДС * Курс / Кратность,2));
			КонецЦикла;	
			СтрокиСчетаФактуры.ЗаполнитьЗначения(0, "Цена");
		КонецЕсли;
		
