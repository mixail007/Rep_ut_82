// функции для совместимости кода 
&НаКлиенте
Функция сбисПолучитьФорму(ИмяФормы)
	Если ТипЗнч(ЭтаФорма) = Тип("УправляемаяФорма") Тогда
		Попытка
			ЭтотОбъект="";
		Исключение
		КонецПопытки;
		Возврат ПолучитьФорму("ВнешняяОбработка.СБИС.Форма."+ИмяФормы);
	КонецЕсли;
	Возврат ЭтотОбъект.ПолучитьФорму(ИмяФормы);
КонецФункции

&НаКлиенте
Процедура ОтправитьДокументы(Кэш,МассивСтрок) Экспорт
	
	фрм = сбисПолучитьФорму("ФормаГлавноеОкно").сбисНайтиФормуФункции("ОтправитьДокументы","Документ_ОказаниеУслуг","Документ_Шаблон",Кэш);
	фрм.ОтправитьДокументы(Кэш,МассивСтрок);
	
КонецПроцедуры

&НаКлиенте
Функция ПрочитатьДокумент(Кэш,Контекст) Экспорт
	
	фрм = сбисПолучитьФорму("ФормаГлавноеОкно").сбисНайтиФормуФункции("ПрочитатьДокумент","Документ_ОказаниеУслуг","Документ_Шаблон",Кэш);
	Возврат фрм.ПрочитатьДокумент(Кэш,Контекст);
	
КонецФункции
&НаКлиенте
Функция ПолучитьТабличнуюЧастьДокумента1С(Кэш,Контекст) Экспорт
	
	фрм = сбисПолучитьФорму("ФормаГлавноеОкно").сбисНайтиФормуФункции("ПолучитьТабличнуюЧастьДокумента1С","Документ_ОказаниеУслуг","Документ_Шаблон",Кэш);
	Возврат фрм.ПолучитьТабличнуюЧастьДокумента1С(Кэш,Контекст);
	
КонецФункции
